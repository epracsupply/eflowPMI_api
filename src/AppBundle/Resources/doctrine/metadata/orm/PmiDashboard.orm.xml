<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PmiDashboard" table="PMI_DASHBOARD">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codigoDashboard" type="string" column="CODIGODASHBOARD" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="titulo" type="string" column="TITULO" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="DESCRIPCION" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="template" type="string" column="TEMPLATE" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
