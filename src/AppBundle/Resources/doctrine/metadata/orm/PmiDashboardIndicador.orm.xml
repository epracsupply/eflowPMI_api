<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PmiDashboardIndicador" table="PMI_DASHBOARD_INDICADOR">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codigoDashboard" type="string" column="CODIGODASHBOARD" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigoIndicador" type="string" column="CODIGOINDICADOR" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipoGrafico" type="string" column="TIPOGRAFICO" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cantidad" type="integer" column="CANTIDAD" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="periodos" type="integer" column="PERIODOS" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="textox" type="string" column="TEXTOX" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="textoy" type="string" column="TEXTOY" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="posicion" type="integer" column="POSICION" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
